{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "resolveJsonModule": true
  },
  "include": [
    "vite.config.ts",
    "src/**/*.tsx",
    "src/**/*.jsx",
    "src/**/*.ts",
    "amplify/**/*ts"
  ]
}
